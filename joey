local function TQEKRP_fake_script() -- X.Script 
	local script = Instance.new('Script', X)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.MainFrame:Destroy()
	end)
end
coroutine.wrap(TQEKRP_fake_script)()
local function QSOK_fake_script() -- Gui.LocalScript 
	local script = Instance.new('LocalScript', Gui)

	local plr = game.Players.LocalPlayer
	
	if plr:IsInGroup(12867018) then
		script.Parent.MainFrame.Visible = true
	else
		script.Parent.MainFrame:Destroy()
		script.Parent.TextLabel.Visible = true
		wait(5)
		plr:Kick("Joey, no more trying to get my gui for free and actually pay for it!!!!")
	end
end
coroutine.wrap(QSOK_fake_script)()
