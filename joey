-- Scripts:

local function VEDMTJT_fake_script() -- MainFrame.Script 
	local script = Instance.new('Script', MainFrame)

	game.StarterGui:SetCore("SendNotification", {
		Title = "Credits to Dark SlayerYT#5012 for making",
		Text = "very hot yes mmm",
		Duration = 5
	})
end
coroutine.wrap(VEDMTJT_fake_script)()
local function LMTY_fake_script() -- MainFrame.DragScript 
	local script = Instance.new('LocalScript', MainFrame)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
	------------------------------------------------------------
	--;-- -Only Change DragSpeed- -;--                         |
	--;----;-- (I Forgot What The Other Stuff Dose) --;----;-- |
	------------------------------------------------------------
end
coroutine.wrap(LMTY_fake_script)()
local function BXKE_fake_script() -- InfShotgunRounds.Script 
	local script = Instance.new('Script', InfShotgunRounds)

	script.Parent.MouseButton1Click:Connect(function()
		_G.bulletsPerRound = 50 -- set to how many bullets you want shot per click
	
	
	
		local lp = game.Players.LocalPlayer
	
	
		local shotgunnames = {}
	
		local itemlist
		local s,e = pcall(function()
			itemlist = loadstring(decompile(game:GetService("ReplicatedStorage"):WaitForChild("Client"):WaitForChild("ItemList")))()
		end)
		if not s then 
			itemlist = loadstring(game:HttpGet("https://raw.githubusercontent.com/BonfireDevelopment/Roblox/main/Anomic/Support%20Code/itemlist.lua%22"))()
		end
		local itemnamelist = {}
	
		for k,v in pairs (itemlist) do
			table.insert(itemnamelist, k)
			if string.find(k, "Shotgun") or k == "Sawed Off" then
				table.insert(shotgunnames, k)
			end
		end
	
		local function ShotgunCheck(mode)
			if mode == "char" then
				if lp.Character and lp.Character:FindFirstChildOfClass("Tool") and table.find(shotgunnames, lp.Character:FindFirstChildOfClass("Tool").Name) then
					return lp.Character:FindFirstChildOfClass("Tool")
				else
					return false
				end
			elseif mode == "backpack" then
				for i,v in pairs (lp.Backpack:GetChildren()) do
					if table.find(shotgunnames, v.Name) then
						return v
					end
				end
				return false
			end
		end
		local miniguntoggle = false
	
		local mouse = lp:GetMouse()
	
		mouse.Button1Down:Connect(function()
			if ShotgunCheck("char") then
				for i=1,_G.bulletsPerRound do
					ShotgunCheck("char").MainGunScript.ShotEvent:Fire(mouse, "Shotgun")
				end
			end
		end)
		local function mma(e, userInputState) 
			if userInputState == Enum.UserInputState.End then
				if miniguntoggle then miniguntoggle = false return end
				miniguntoggle = true
				if not ShotgunCheck("char") then return end
				if not mouse1click then return end
				while miniguntoggle do
					if not ShotgunCheck("char") then miniguntoggle = false end
					mouse1click()
					task.wait(0.1)
				end
			end
		end
	
		game.ContextActionService:BindActionAtPriority("MiddleMouseAction", mma, false, 100000, Enum.UserInputType.MouseButton3)
	
		game.StarterGui:SetCore("SendNotification", {
			Title = "Credits to Bonfire#9355",
			Text = "My dad raped me",
			Duration = 5
		})
	
		print(hookfunction())
	end)
end
coroutine.wrap(BXKE_fake_script)()
local function TYED_fake_script() -- AntiCheatbypass.Script 
	local script = Instance.new('Script', AntiCheatbypass)

	script.Parent.MouseButton1Click:Connect(function()
		if not game:IsLoaded() then game.Loaded:Wait() end
		local vu = game:GetService("VirtualUser")
		game:GetService("Players").LocalPlayer.Idled:connect(function()
			vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
			wait(1)
			vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
		end)
	
		local mt = getrawmetatable(game)
		local old = mt.__namecall
		local protect = newcclosure or protect_function
	
		setreadonly(mt, false)
		mt.__namecall = protect(function(self, ...)
			local method = getnamecallmethod()
			if method == "Kick" then
				wait(9e9)
				return
			end
			return old(self, ...)
		end)
		hookfunction(game.Players.LocalPlayer.Kick,protect(function() wait(9e9) end))
	end)
end
coroutine.wrap(TYED_fake_script)()
