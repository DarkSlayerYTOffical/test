local function WTTPVD_fake_script() -- ShortJotaro.Script 
	local script = Instance.new('Script', ShortJotaro)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService('Players')
		local LocalPlayer = Players.LocalPlayer or Players.LocalPlayer:Wait()
		local Character = LocalPlayer.Character or LocalPlayer.Character:Wait()
		local Humanoid = Character:WaitForChild('Humanoid')
		local FireSequences = {{'Cloth';'Cool Hair'};{'Cloth';'No Mask'};{'Cloth';'No Beard'};{'Cloth';'Brim Hat'};{'Cloth';'No Jewelry'};{'Color';Color3.fromRGB(    255, 219, 172);'SkinColor'};{'Cloth';'No Glasses'};{'CustomCloth';'329801167'};{'CustomCloth';'329801109'}}
		coroutine.wrap(function() 
			for _, Sequence in next, FireSequences do
				game:GetService("ReplicatedStorage")["_CS.Events"].EquipAvatarItem:FireServer(unpack(Sequence))
			end
		end)()
		_G.isdel = false
	
		function Main(Delay)
			local H = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid')
			local function DeleteOriginal()
				for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
					if v.Name == 'OriginalSize' then
						v:Destroy()
					end
					if v.Name == 'OriginalPosition' then 
						v:Destroy()
					end
				end
			end
			local function Width()
				wait(Delay)
				DeleteOriginal()
				H:FindFirstChild("BodyWidthScale"):Destroy()
			end
			local function Depth()
				wait(Delay)
				DeleteOriginal()
				H:FindFirstChild("BodyDepthScale"):Destroy()
			end
			local function Head()
				wait(Delay)
				DeleteOriginal()
				H:FindFirstChild("HeadScale"):Destroy()
			end
			local function Type()
				wait(Delay)
				DeleteOriginal()
				H:FindFirstChild("BodyTypeScale"):Destroy()
			end 
			--// Rearrage these below to customize titan
			Width()
			Type()
			Depth()
			Head()
			--\
		end
		Main(0.8)
	end)
end
coroutine.wrap(WTTPVD_fake_script)()
local function EJSP_fake_script() -- ShortStarPlatinum.Script 
	local script = Instance.new('Script', ShortStarPlatinum)

	script.Parent.MouseButton1Click:Connect(function()
		-- Star Platinum
	
		game:GetService("ReplicatedStorage")["_CS.Events"].EquipAvatarItem:FireServer("Color",Color3.new(128,0,128),"SkinColor")
		game:GetService("ReplicatedStorage")["_CS.Events"].EquipAvatarItem:FireServer("CustomCloth",6644731266)
		game:GetService("ReplicatedStorage")["_CS.Events"].EquipAvatarItem:FireServer("CustomCloth",6644731024)
	
		local Players = game:GetService('Players')
		local LocalPlayer = Players.LocalPlayer or Players.LocalPlayer:Wait()
		local Character = LocalPlayer.Character or LocalPlayer.Character:Wait()
		local Humanoid = Character:WaitForChild('Humanoid')
		local FireSequences = {{'Cloth';'Bed Hair'};{'Cloth';'No Mask'};{'Cloth';'No Beard'};{'Cloth';'No Hat'};{'Cloth';'No Jewelry'};{'Color';Color3.fromRGB(    128,0,128);'SkinColor'};{'Cloth';'No Glasses'};{'CustomCloth';'6644731266'};{'CustomCloth';'6644731024'}}
		coroutine.wrap(function() 
			for _, Sequence in next, FireSequences do
				game:GetService("ReplicatedStorage")["_CS.Events"].EquipAvatarItem:FireServer(unpack(Sequence))
			end
		end)()
		_G.isdel = false
	
		function Main(Delay)
			local H = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid')
			local function DeleteOriginal()
				for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
					if v.Name == 'OriginalSize' then
						v:Destroy()
					end
					if v.Name == 'OriginalPosition' then 
						v:Destroy()
					end
				end
			end
			local function Width()
				wait(Delay)
				DeleteOriginal()
				H:FindFirstChild("BodyWidthScale"):Destroy()
			end
			local function Depth()
				wait(Delay)
				DeleteOriginal()
				H:FindFirstChild("BodyDepthScale"):Destroy()
			end
			local function Head()
				wait(Delay)
				DeleteOriginal()
				H:FindFirstChild("HeadScale"):Destroy()
			end
			local function Type()
				wait(Delay)
				DeleteOriginal()
				H:FindFirstChild("BodyTypeScale"):Destroy()
			end 
			--// Rearrage these below to customize titan
			Width()
			Type()
			Depth()
			Head()
			--\
		end
		Main(0.8)
	end)
end
