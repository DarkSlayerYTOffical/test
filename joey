-- Scripts:

local function JSUGM_fake_script() -- MainButton.LocalScript 
	local script = Instance.new('LocalScript', MainButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.MainFrame.Visible = true
		script.Parent.Parent.AnimationsFrame.Visible = false
		script.Parent.Parent.SignsFrame.Visible = false
		script.Parent.Parent.MiscFrame.Visible = false
		script.Parent.Parent.OtherScriptsFrame.Visible = false
	end)
	
end
coroutine.wrap(JSUGM_fake_script)()
local function QXDV_fake_script() -- SignsButton.LocalScript 
	local script = Instance.new('LocalScript', SignsButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.MainFrame.Visible = false
		script.Parent.Parent.AnimationsFrame.Visible = false
		script.Parent.Parent.SignsFrame.Visible =true
		script.Parent.Parent.MiscFrame.Visible = false
		script.Parent.Parent.OtherScriptsFrame.Visible = false
	end)
	
end
